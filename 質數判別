#include<stdio.h>  
int main(){  
    int i,j,flag=0;  
    while(scanf("%d",&i)!=EOF){  
        flag=0;  
            for(j=2;j<i;j++){  
                if(i%j==0){  
                    printf("NO\n");  
                    break;  
                }  
                if(j==i-1) {printf("YES\n");  
                flag=1;  
                break;}  
            }  
    }  
    return 0;  
} 
