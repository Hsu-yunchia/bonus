#include <stdio.h>        
int main(){        
    int a,b;      
    scanf("%d %d",&a,&b);        
    printf("%d+%d=%d\n",a,b,a+b);      
    printf("%d*%d=%d\n",a,b,a*b);      
    printf("%d-%d=%d\n",a,b,a-b);      
    if(b==0){return 0;}  
    else{      
    if(a-b*(a/b)>=0){      
    printf("%d/%d=%d...%d\n",a,b,a/b,a-b*(a/b));      
    }      
    else {if(a<0&&b<0){  
        printf("%d/%d=%d...%d\n",a,b,a/b,b*(a/b)-a);  
        }  
        if(a<0&&b>0){  
            printf("%d/%d=%d...%d\n",a,b,a/b-1,a-b*(a/b-1));  
            }  
    }     
    }      
    return 0;        
}  
