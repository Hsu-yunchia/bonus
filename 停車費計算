#include<stdio.h>  
int main(){  
    int sh,sm,eh,em,time;  
    scanf("%d %d",&sh,&sm);  
    scanf("%d %d",&eh,&em);  
        time=(eh-sh)*60+(em-sm);  
        if(time<121){  
            printf("%d\n",30*(time/30));  
        }  
        if(120<time&&time<241){  
            printf("%d\n",120+(time-120)/30*40);  
        }  
        if(time>240){  
            printf("%d\n",280+(time-240)/30*60);  
        }  
    return 0;  
}
