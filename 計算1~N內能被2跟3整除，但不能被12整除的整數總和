#include<stdio.h>    
int main(){    
    int a,i,ans=0;    
    while(scanf("%d",&a)!=EOF){    
        ans=0;  
        for(i=1;i<=a;i++){  
            if(i%2==0&&i%3==0&&i%12!=0){  
                ans=ans+i;  
            }  
        }  
        printf("%d\n",ans);  
    }    
    return 0;    
} 
