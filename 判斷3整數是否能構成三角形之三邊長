#include<stdio.h>    
int main(){    
    int x,y,z;    
    while(scanf("%d %d %d",&x,&y,&z)!=EOF){  
        if(x+y<=z){  
            printf("unfit\n");  
        }  
        else if(x+z<=y){  
            printf("unfit\n");  
        }  
        else if(y+z<=x){  
            printf("unfit\n");  
        }  
        else printf("fit\n");  
    }  
    return 0;  
}  
